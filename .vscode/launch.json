    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/debug/cbs",
            "args": ["-i", "${workspaceFolder}/benchmark/32x32_obst204/map_32by32_obst204_agents10_ex1.yaml", "-o", "/home/hadar/My_Project_MAPF/debug/output.yaml", 
                "--secondOutput","/home/hadar/My_Project_MAPF/debug/second_output.yaml",
            "--approach", "naive", "--agentNumber", "9", "--timeStep", "4", "--x", "25", "--y", "20"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}